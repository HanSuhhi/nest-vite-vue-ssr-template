{
  "name": "nest-vite-vue-ssr",
  "private": true,
  "version": "0.0.0",
  "author": "Don",
  "keywords": [
    "nestjs",
    "vite",
    "vitejs",
    "vue",
    "ssr",
    "nest"
  ],
  "scripts": {
    "dev": "pnpm ssr:dev",
    "csr:dev": "vite",
    "preview": "pnpm ssr:preview",
    "ssr:dev": "nest start -w",
    "ssr:preview": "NODE_ENV=production node dist/server/main.js",
    "build": "pnpm build:backend & pnpm build:frontend & pnpm build:server",
    "build:backend": "nest build",
    "build:frontend": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/middleware --ssr src/entry-server.ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@types/lodash": "^4.14.195",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "vue": "^3.2.47",
    "vue-router": "4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "compression": "^1.7.4",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fast-glob": "^3.2.12",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vue-tsc": "^1.4.2"
  }
}